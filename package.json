{
  "name": "slt-app",
  "version": "0.0.1",
  "description": "simple label tool",
  "author": "lengjingify",
  "keywords": [
    "label tool",
    "ai"
  ],
  "scripts": {
    "start": "parcel index.html --no-source-maps",
    "clean": "rm -rf lib",
    "build": "npm run clean && npm run build:ts && npm run build:bundle && cp ./package.json ./lib && cp ./README.md ./lib",
    "build:ts": "tsc",
    "build:bundle": "parcel build --dist-dir lib --no-source-maps ./src/slt.bundle.tsx"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "parcel": "^2.9.3",
    "process": "^0.11.10"
  },
  "dependencies": {
    "mobx": "^6.9.0",
    "mobx-state-tree": "^5.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3"
  },
  "main": "./lib/slt.js"
}
