{
  "name": "slt-app",
  "version": "0.0.1",
  "description": "simple label tool",
  "author": "lengjingify",
  "keywords": [
    "label tool",
    "ai"
  ],
  "scripts": {
    "start": "parcel index.html",
    "build": "npm run build:src && npm run build:bundle && npm build:gh-pages",
    "build:src": "rm -rf lib && tsc",
    "build:bundle": "rm -f lib/slt.bundle.js && parcel build --no-cache --target bundle",
    "build:gh-pages": "rm -rf dist && parcel build --no-cache --public-url /simple-label-tool --target .",
    "publish:gh-pages": "gh-pages -d dist"
  },
  "devDependencies": {
    "@parcel/transformer-less": "2.9.3",
    "@types/color": "^3.0.3",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/three": "^0.152.1",
    "gh-pages": "^5.0.0",
    "parcel": "^2.9.3",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.6",
    "@tweenjs/tween.js": "^21.0.0",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "konva": "^9.2.0",
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "mobx-state-tree": "^5.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "three": "^0.153.0",
    "typescript": "^5.1.3"
  },
  "main": "./lib/slt.js",
  "targets": {
    "bundle": {
      "source": "src/slt.bundle.tsx",
      "distDir": "lib",
      "optimize": false,
      "sourceMap": false
    },
    ".": {
      "source": "index.html"
    }
  }
}
